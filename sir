javascript:(function(){let e=document.createElement('div');e.style.cssText='position:fixed;bottom:0;left:0;width:100%;height:auto;max-height:80vh;background:#000;color:#0f0;z-index:999999;padding:20px;overflow-y:auto;font-family:"Courier%20New",%20monospace;box-shadow:0%200%2020px%20rgba(0,255,0,0.5);border-top:2px%20solid%20#0f0;';e.innerHTML='<div%20style="position:absolute;top:10px;right:10px;"><button%20id="closeBtn"%20style="background:#ff4444;color:white;border:none;padding:5px%2010px;cursor:pointer;border-radius:50%;box-shadow:0%200%2010px%20rgba(255,0,0,0.5);">X</button></div><h3%20style="color:#0f0;margin-bottom:15px;text-shadow:0%200%2010px%20#0f0;">%F0%9F%91%81%EF%B8%8F%20Third%20EyE%20-%20Advanced%20Vulnerability%20Scanner</h3><div%20style="margin-bottom:15px;display:flex;flex-wrap:wrap;gap:10px;"><button%20id="vulnerableBtn"%20style="background:#ff9900;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(255,153,0,0.5);">Show%20Vulnerable%20URLs</button><button%20id="allUrlsBtn"%20style="background:#00cc66;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(0,204,102,0.5);">Show%20All%20URLs</button><button%20id="cloneBtn"%20style="background:#0099ff;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(0,153,255,0.5);">Clone%20URLs</button><button%20id="downloadBtn"%20style="background:#6633ff;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(102,51,255,0.5);">Download%20URLs</button><button%20id="serverSideBtn"%20style="background:#ff3399;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(255,51,153,0.5);">Server-Side%20Vuln%20Test</button><button%20id="clientSideBtn"%20style="background:#ff33cc;color:black;border:none;padding:5px%2010px;cursor:pointer;border-radius:5px;box-shadow:0%200%2010px%20rgba(255,51,204,0.5);">Client-Side%20Vuln%20Test</button></div><div%20id="results"%20style="background:#111;padding:10px;border-radius:5px;overflow-y:auto;max-height:60vh;color:#0f0;text-shadow:0%200%205px%20#0f0;border:1px%20solid%20#0f0;">Scanning...</div><div%20id="pageChangeStatus"%20style="margin-top:10px;color:#0f0;text-shadow:0%200%205px%20#0f0;"></div>';document.body.appendChild(e);let%20currentDomain=window.location.hostname;let%20allUrls=[];let%20lastUrl=window.location.href;function%20findUrls(){console.log("Finding%20URLs...");let%20urls=[];let%20sources=[...document.getElementsByTagName('a'),...document.getElementsByTagName('script'),...document.getElementsByTagName('img'),...document.getElementsByTagName('link'),...document.getElementsByTagName('form')];sources.forEach(element=>{if(element.href)urls.push(element.href);if(element.src)urls.push(element.src);if(element.action)urls.push(element.action);});let%20content=document.documentElement.innerHTML;let%20urlPattern=/(?:url\(|href="|src="|action="|url:|endpoint:|path:|route:)\s*['"]?([^'"\)\s>]+)/gi;let%20match;while((match=urlPattern.exec(content))!==null){if(match[1]&&!match[1].startsWith('data:'))urls.push(match[1]);}let%20scriptPattern=/"[^"]*"|'[^']*'/g;let%20scripts=document.documentElement.innerHTML.match(scriptPattern)||[];scripts.forEach(script=>{let%20urlMatches=script.match(/(?:\/[a-zA-Z0-9_-]+)+(?:\.[a-zA-Z0-9]+)?/g)||[];urlMatches.forEach(url=>urls.push(url));});performance.getEntriesByType('resource').forEach(entry=>urls.push(entry.name));return[...new%20Set(urls)];}function%20filterVulnerableUrls(urls){console.log("Filtering%20vulnerable%20URLs...");let%20vulnerablePatterns=[{pattern:/api\//i,reason:"API%20Endpoint%20-%20Potential%20IDOR,%20Broken%20Authentication,%20or%20Data%20Exposure",type:"API"},{pattern:/v[0-9]+\//i,reason:"Versioned%20API%20Endpoint%20-%20Potential%20IDOR,%20Broken%20Authentication,%20or%20Data%20Exposure",type:"API"},{pattern:/\.php/i,reason:"PHP%20File%20-%20Potential%20Remote%20Code%20Execution%20(RCE)%20or%20File%20Inclusion",type:"File%20Inclusion"},{pattern:/\.asp/i,reason:"ASP%20File%20-%20Potential%20Injection%20or%20File%20Inclusion",type:"File%20Inclusion"},{pattern:/\.action/i,reason:"Action%20File%20-%20Potential%20Logic%20Flaws%20or%20Injection",type:"Injection"},{pattern:/\.do/i,reason:"DO%20File%20-%20Potential%20Logic%20Flaws%20or%20Injection",type:"Injection"},{pattern:/\.jsp/i,reason:"JSP%20File%20-%20Potential%20Remote%20Code%20Execution%20(RCE)%20or%20File%20Inclusion",type:"File%20Inclusion"},{pattern:/\.ashx/i,reason:"ASHX%20File%20-%20Potential%20Logic%20Flaws%20or%20Injection",type:"Injection"},{pattern:/\.asmx/i,reason:"ASMX%20File%20-%20Potential%20Logic%20Flaws%20or%20Injection",type:"Injection"},{pattern:/\.json/i,reason:"JSON%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.xml/i,reason:"XML%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.git/i,reason:"Git%20Directory%20-%20Potential%20Source%20Code%20Leakage",type:"Data%20Exposure"},{pattern:/\.env/i,reason:"Environment%20File%20-%20Potential%20API%20Keys%20or%20Secrets%20Exposure",type:"Data%20Exposure"},{pattern:/\.bak/i,reason:"Backup%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.swp/i,reason:"Swap%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.sql/i,reason:"SQL%20File%20-%20Potential%20Database%20Dump%20or%20Sensitive%20Queries",type:"Data%20Exposure"},{pattern:/\.config/i,reason:"Config%20File%20-%20Potential%20Sensitive%20Settings%20Exposure",type:"Data%20Exposure"},{pattern:/\.htaccess/i,reason:"HTAccess%20File%20-%20Potential%20Server%20Misconfiguration",type:"Misconfiguration"},{pattern:/\.htpasswd/i,reason:"HTPasswd%20File%20-%20Potential%20Unauthorized%20Access",type:"Misconfiguration"},{pattern:/admin/i,reason:"Admin%20Panel%20-%20Potential%20Broken%20Access%20Control",type:"Access%20Control"},{pattern:/login/i,reason:"Login%20Page%20-%20Potential%20Authentication%20Bypass",type:"Authentication"},{pattern:/register/i,reason:"Registration%20Page%20-%20Potential%20Account%20Takeover",type:"Authentication"},{pattern:/upload/i,reason:"File%20Upload%20-%20Potential%20Unrestricted%20File%20Upload",type:"File%20Upload"},{pattern:/download/i,reason:"File%20Download%20-%20Potential%20Arbitrary%20File%20Download",type:"File%20Download"},{pattern:/debug/i,reason:"Debug%20Endpoint%20-%20Potential%20Information%20Disclosure",type:"Information%20Disclosure"},{pattern:/test/i,reason:"Test%20Endpoint%20-%20Potential%20Information%20Disclosure",type:"Information%20Disclosure"},{pattern:/backup/i,reason:"Backup%20Directory%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.yaml/i,reason:"YAML%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.ini/i,reason:"INI%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\.log/i,reason:"Log%20File%20-%20Potential%20Sensitive%20Data%20Exposure",type:"Data%20Exposure"},{pattern:/\?[a-zA-Z0-9_]+=/i,reason:"URL%20Parameter%20-%20Potential%20SQL%20Injection%20or%20XSS",type:"Injection"}];let%20vulnerableUrls=[];urls.forEach(url=>{let%20reasons=[];vulnerablePatterns.forEach(pattern=>{if(pattern.pattern.test(url)){reasons.push({reason:pattern.reason,type:pattern.type});}});if(reasons.length>0){vulnerableUrls.push({url:url,reasons:reasons});}});return%20vulnerableUrls;}function%20displayUrls(urls,isVulnerable=false){console.log("Displaying%20URLs...");if(isVulnerable){document.getElementById('results').innerHTML=`<div%20style="margin:10px%200;color:#0f0;text-shadow:0%200%2010px%20#0f0;">%E2%9C%85%20Found%20${urls.length}%20Vulnerable%20URLs%20on%20${currentDomain}</div><div%20style="background:#111;padding:10px;border-radius:5px;overflow-y:auto;max-height:60vh;color:#0f0;text-shadow:0%200%205px%20#0f0;border:1px%20solid%20#0f0;">${urls.map(url=>`<div%20style="color:#0f0;margin:5px%200;padding:5px;background:#222;border-radius:3px;word-break:break-all;border:1px%20solid%20#0f0;cursor:pointer;"%20onclick="copyToClipboard('${url.url.replace(/'/g,%20"\\'")}')"><div>${url.url}</div>${url.reasons?url.reasons.map(reason=>`<div%20style="color:${reason.type==='Injection'?'#ff4444':reason.type==='Data%20Exposure'?'#ff9900':reason.type==='Authentication'?'#00cc66':'#0099ff'};font-size:12px;text-shadow:0%200%205px%20${reason.type==='Injection'?'#ff4444':reason.type==='Data%20Exposure'?'#ff9900':reason.type==='Authentication'?'#00cc66':'#0099ff'};">${reason.reason}</div>`).join(''):''}</div>`).join('')}</div>`;}else{document.getElementById('results').innerHTML=`<div%20style="margin:10px%200;color:#0f0;text-shadow:0%200%2010px%20#0f0;">%E2%9C%85%20Found%20${urls.length}%20URLs%20on%20${currentDomain}</div><div%20style="background:#111;padding:10px;border-radius:5px;overflow-y:auto;max-height:60vh;color:#0f0;text-shadow:0%200%205px%20#0f0;border:1px%20solid%20#0f0;">${urls.map(url=>`<div%20style="color:#0f0;margin:5px%200;padding:5px;background:#222;border-radius:3px;word-break:break-all;border:1px%20solid%20#0f0;cursor:pointer;"%20onclick="copyToClipboard('${url.replace(/'/g,%20"\\'")}')">${url}</div>`).join('')}</div>`;}}function%20copyToClipboard(text){let%20textarea=document.createElement('textarea');textarea.value=text;document.body.appendChild(textarea);textarea.select();try{document.execCommand('copy');alert(`Copied%20to%20clipboard:%20${text}`);}catch(err){alert("Failed%20to%20copy%20text.");}document.body.removeChild(textarea);}function%20downloadUrls(urls,filename){console.log("Downloading%20URLs...");let%20blob=new%20Blob([urls.join('\n')],{type:'text/plain'});let%20a=document.createElement('a');a.href=URL.createObjectURL(blob);a.download=filename;document.body.appendChild(a);a.click();document.body.removeChild(a);URL.revokeObjectURL(a.href);}function%20testServerSideVulns(urls){console.log("Testing%20Server-Side%20Vulnerabilities...");let%20vulnerableUrls=filterVulnerableUrls(urls);if(vulnerableUrls.length===0){console.log("No%20Server-Side%20Vulnerabilities%20found.");document.getElementById('results').innerHTML=`<div%20style="margin:10px%200;color:#0f0;text-shadow:0%200%2010px%20#0f0;">%E2%9D%8C%20No%20Server-Side%20Vulnerabilities%20Found%20on%20${currentDomain}</div>`;}else{console.log("Found%20Server-Side%20Vulnerabilities:",%20vulnerableUrls);displayUrls(vulnerableUrls,true);}}function%20testClientSideVulns(){console.log("Testing%20Client-Side%20Vulnerabilities...");let%20vulnerableElements=[];let%20scripts=document.querySelectorAll('script');scripts.forEach(script=>{if(script.src&&!script.src.includes(currentDomain)){vulnerableElements.push({element:script.src,reason:"External%20Script%20-%20Potential%20XSS%20or%20Data%20Exposure",type:"Client-Side"});}});let%20forms=document.querySelectorAll('form');forms.forEach(form=>{if(!form.action||!form.querySelector('input[name="csrf_token"]')){vulnerableElements.push({element:form.outerHTML,reason:"Form%20without%20CSRF%20Token%20-%20Potential%20CSRF%20Attack",type:"Client-Side"});}});if(vulnerableElements.length===0){console.log("No%20Client-Side%20Vulnerabilities%20found.");document.getElementById('results').innerHTML=`<div%20style="margin:10px%200;color:#0f0;text-shadow:0%200%2010px%20#0f0;">%E2%9D%8C%20No%20Client-Side%20Vulnerabilities%20Found%20on%20${currentDomain}</div>`;}else{console.log("Found%20Client-Side%20Vulnerabilities:",%20vulnerableElements);document.getElementById('results').innerHTML=`<div%20style="margin:10px%200;color:#0f0;text-shadow:0%200%2010px%20#0f0;">%E2%9C%85%20Found%20${vulnerableElements.length}%20Client-Side%20Vulnerabilities%20on%20${currentDomain}</div><div%20style="background:#111;padding:10px;border-radius:5px;overflow-y:auto;max-height:60vh;color:#0f0;text-shadow:0%200%205px%20#0f0;border:1px%20solid%20#0f0;">${vulnerableElements.map(element=>`<div%20style="color:#0f0;margin:5px%200;padding:5px;background:#222;border-radius:3px;word-break:break-all;border:1px%20solid%20#0f0;"><div>${element.element}</div><div%20style="color:#ff3399;font-size:12px;text-shadow:0%200%205px%20#ff3399;">${element.reason}</div></div>`).join('')}</div>`;}}function%20highlightButton(button){document.querySelectorAll('button').forEach(btn=>{btn.style.boxShadow='0%200%2010px%20rgba(0,0,0,0.5)';btn.style.border='none';});button.style.boxShadow='0%200%2020px%20rgba(0,255,0,0.8)';button.style.border='2px%20solid%20#0f0';}function%20checkPageChange(){if(window.location.href!==lastUrl){document.getElementById('pageChangeStatus').innerHTML='%F0%9F%94%84%20Page%20has%20changed!';lastUrl=window.location.href;}else{document.getElementById('pageChangeStatus').innerHTML='';}}allUrls=findUrls();allUrls.sort();displayUrls(allUrls);document.getElementById('vulnerableBtn').addEventListener('click',()=>{console.log("Show%20Vulnerable%20URLs%20button%20clicked.");highlightButton(document.getElementById('vulnerableBtn'));let%20vulnerableUrls=filterVulnerableUrls(allUrls);displayUrls(vulnerableUrls,true);});document.getElementById('allUrlsBtn').addEventListener('click',()=>{console.log("Show%20All%20URLs%20button%20clicked.");highlightButton(document.getElementById('allUrlsBtn'));displayUrls(allUrls);});document.getElementById('cloneBtn').addEventListener('click',()=>{console.log("Clone%20URLs%20button%20clicked.");highlightButton(document.getElementById('cloneBtn'));let%20displayedUrls=Array.from(document.querySelectorAll('#results%20div%20div')).map(div=>div.textContent);navigator.clipboard.writeText(displayedUrls.join('\n')).then(()=>alert('URLs%20copied%20to%20clipboard!')).catch(()=>alert('Failed%20to%20copy%20URLs.'));});document.getElementById('downloadBtn').addEventListener('click',()=>{console.log("Download%20URLs%20button%20clicked.");highlightButton(document.getElementById('downloadBtn'));let%20displayedUrls=Array.from(document.querySelectorAll('#results%20div%20div')).map(div=>div.textContent);let%20filename=currentDomain+'_urls.txt';downloadUrls(displayedUrls,filename);});document.getElementById('serverSideBtn').addEventListener('click',()=>{console.log("Server-Side%20Vuln%20Test%20button%20clicked.");highlightButton(document.getElementById('serverSideBtn'));testServerSideVulns(allUrls);});document.getElementById('clientSideBtn').addEventListener('click',()=>{console.log("Client-Side%20Vuln%20Test%20button%20clicked.");highlightButton(document.getElementById('clientSideBtn'));testClientSideVulns();});document.getElementById('closeBtn').addEventListener('click',()=>{console.log("Close%20button%20clicked.");e.remove();});setInterval(checkPageChange,1000);})();
